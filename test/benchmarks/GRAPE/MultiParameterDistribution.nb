      ",", " ", 
     RowBox[{"uniformDistribution", "[", 
      RowBox[{
       SubscriptBox["\[Delta]B", "1"], ",", " ", 
       RowBox[{"-", "1"}], ",", " ", "1", ",", " ", "numberOfPoints"}], 
      "]"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]], "Code",
