   RowBox[{"ProductParameterDistribution", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"uniformDistribution", "[", 
      RowBox[{
       SubscriptBox["\[Delta]B", "0"], ",", " ", 
