Cell[TextData[{
 "When d>2, there is a good chance a random unitary will have eigenvalues \
that have the origin in their convex hull, in which case the diamond norm is \
exactly 2 because the smallest enclosing circle ",
 StyleBox["is",
  FontSlant->"Italic"],
 " the unit circle. If the origin is not contained in the convex hull of the \
eigenvalues, the minimum enclosing circle touches only two of the eigenvalues."
}], "Text",
 CellChangeTimes->{{3.65849426713374*^9, 3.6584943322462673`*^9}, {
  3.658494670387967*^9, 3.658494765523479*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"d", "=", "3"}], ",", "U", ",", "chull", ",", "nearest"}], "}"}],
    ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"U", "=", 
     RowBox[{"RandomUnitary", "[", "d", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "@", 
     RowBox[{"DiamondNormDistance", "[", "U", "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"chull", "=", 
     RowBox[{"ConvexHullMesh", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Re", "@", "#"}], ",", 
          RowBox[{"Im", "@", "#"}]}], "}"}], "&"}], "/@", 
       RowBox[{"Eigenvalues", "[", "U", "]"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"nearest", "=", 
     RowBox[{"RegionNearest", "[", 
