     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"\"\<Not TP but CP symbolic:\>\"", ",", 
      RowBox[{"UnitaryQ", "[", 
       RowBox[{
        RowBox[{"Kraus", "[", 
         RowBox[{"{", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"a", ",", "b"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", 
               RowBox[{"b", "\[Conjugate]"}]}], ",", 
              RowBox[{"a", "\[Conjugate]"}]}], "}"}]}], "}"}], "}"}], "]"}], 
        ",", 
        RowBox[{"Assumptions", "\[Rule]", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            RowBox[{"Abs", "[", "a", "]"}], "2"], "+", 
           SuperscriptBox[
            RowBox[{"Abs", "[", "b", "]"}], "2"]}], "\[Equal]", "2"}]}]}], 
       "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"\"\<General Pauli channel:\>\"", ",", 
      RowBox[{"UnitaryQ", "[", 
       RowBox[{"Chi", "[", 
        RowBox[{"DiagonalMatrix", "[", 
         RowBox[{"{", 
          RowBox[{"p0", ",", "p1", ",", "p2", ",", "p3"}], "}"}], "]"}], 
        "]"}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"\"\<Numerical Mixed Unitary:\>\"", ",", 
